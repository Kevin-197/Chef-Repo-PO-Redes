$TTL 604800
@ IN SOA nameserver.google.com. root.nameserver.google.com. (
                              2         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
@ IN NS nameserver.google.com.
@ IN A 0.0.0.0
@ IN A 0.0.0.0
www IN A 0.0.0.0
www IN A 0.0.0.0